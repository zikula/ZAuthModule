{
    "name": "zikula/zauth-module",
    "version": "3.1.0-dev",
    "description": "Native zikula authentication",
    "type": "zikula-system-module",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Zikula Team",
            "homepage": "https://ziku.la/"
        }
    ],
    "autoload": {
        "psr-4": { "Zikula\\ZAuthModule\\": "" }
    },
    "require": {
        "php": ">=7.2.5",
        "symfony/config": "^5.2",
        "symfony/console": "^5.2",
        "symfony/contracts": "^5.2",
        "symfony/dotenv": "^5.2",
        "symfony/event-dispatcher": "^5.2",
        "symfony/form": "^5.2",
        "symfony/http-foundation": "^5.2",
        "symfony/http-kernel": "^5.2",
        "symfony/mailer": "^5.2",
        "symfony/rate-limiter": "^5.2",
        "symfony/routing": "^5.2",
        "symfony/security": "^5.2",
        "symfony/string": "^5.2",
        "symfony/validator": "^5.2",

        "zikula/core-bundle": "self.version",
        "zikula/hook-bundle": "self.version",
        "zikula/extensions-module": "self.version",
        "zikula/groups-module": "self.version",
        "zikula/menu-module": "self.version",
        "zikula/permissions-module": "self.version",
        "zikula/theme-module": "self.version",
        "zikula/users-module": "self.version",
        "zikula/sortable-columns": "1.*",

        "ircmaxell/random-lib": "1.*"
    },
    "extra": {
        "zikula": {
            "class": "Zikula\\ZAuthModule\\ZikulaZAuthModule",
            "core-compatibility": ">=3.0.0",
            "displayname": "Zikula Native Authorization",
            "url": "zauth",
            "icon": "fas fa-user-lock",
            "capabilities": {
                "admin": {
                    "route": "zikulazauthmodule_useradministration_listmappings"
                }
            },
            "securityschema": {
                "ZikulaZAuthModule::": "::"
            }
        }
    }
}
